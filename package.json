{
  "name": "sofa-odm",
  "version": "0.1.2",
  "description": "NodeJS ODM for Couchbase",
  "main": "main.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "node ./node_modules/mocha/bin/mocha -t 10000",
    "coverage": "node ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec",
    "doc": "node ./node_modules/jsdox/bin/jsdox --output doc/api main.js src && node ./node_modules/jsdox/bin/jsdox --output doc/api/behaviours src/behaviours && node ./node_modules/jsdox/bin/jsdox --output doc/api/validators src/validators"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ichiriac/sofa-odm.git"
  },
  "keywords": [
    "couchbase",
    "odm",
    "crud",
    "unique",
    "index",
    "view",
    "validators"
  ],
  "author": "Ioan CHIRIAC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ichiriac/sofa-odm/issues"
  },
  "homepage": "http://ichiriac.github.io/sofa-odm",
  "dependencies": {
    "q": "^1.0.1",
    "couchbase": "^1.2.4",
    "extend": "^1.3.0",
    "node-uuid": "^1.4.1"
  },
  "devDependencies": {
    "mocha": "^1.21.4",
    "istanbul": "^0.3.0",
    "jsdox": "^0.4.2"
  }
}
